# Kakao Map JavaScript API Key
# https://developers.kakao.com/ 에서 발급
NEXT_PUBLIC_KAKAO_MAP_API_KEY=your_kakao_map_api_key_here

# Public Data Portal API Key (농림축산검역본부 동물병원 데이터)
# https://www.data.go.kr 에서 발급
PUBLIC_DATA_API_KEY=your_public_data_api_key_here

# Google AdSense Client ID
NEXT_PUBLIC_GOOGLE_ADSENSE_CLIENT_ID=ca-pub-xxxxxxxxxxxxxxxx

# Base URL (프로덕션 배포 URL)
# 로컬 개발: http://localhost:3011
# 프로덕션: https://ko-vet-spot.vercel.app
NEXT_PUBLIC_BASE_URL=https://ko-vet-spot.vercel.app

# Environment
NODE_ENV=development

# Optional: Mock data toggle for development
USE_MOCK_DATA=false
